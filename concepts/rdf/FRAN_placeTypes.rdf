    <skos:Concept rdf:about="placeType/montagne">
        <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#PlaceType"/>
        <skos:inScheme rdf:resource="placeTypes"/>
        <skos:prefLabel xml:lang="fr">montagne</skos:prefLabel>
        <skos:altLabel xml:lang="fr">mont</skos:altLabel>
        <skos:broader rdf:resource="placeType/zone%20naturelle"/>
        <skos:definition xml:lang="fr">Forme topographique de relief positif, à la surface de
            planètes telluriques, et faisant partie d'un ensemble — une chaîne de montagnes — ou
            formant un relief isolé (source : Wikipédia).</skos:definition>
        <rdfs:seeAlso rdf:resource="https://fr.wikipedia.org/wiki/Montagne"/>
        <skos:exactMatch rdf:resource="http://data.ign.fr/id/codes/topo/typederelief/Montagne"/>
