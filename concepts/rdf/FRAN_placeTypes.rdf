    <skos:Concept rdf:about="placeType/plateau">
        <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#PlaceType"/>
        <skos:inScheme rdf:resource="placeTypes"/>
        <skos:prefLabel xml:lang="fr">plateau</skos:prefLabel>
        <skos:broader rdf:resource="placeType/zone%20naturelle"/>
        <skos:definition xml:lang="fr">Aire géographique où les cours d’eau sont encaissés, par
            opposition aux plaines où les cours d’eau coulent à fleur de sol. Le relief y est peu
            marqué, ce qui différencie un plateau d’une montagne (source :
            Wikipédia).</skos:definition>
        <rdfs:seeAlso rdf:resource="https://fr.wikipedia.org/wiki/Plateau_(g%C3%A9ographie)"/>
        <!-- <owl:sameAs rdf:resource="https://www.wikidata.org/wiki/Q75520"/>-->
