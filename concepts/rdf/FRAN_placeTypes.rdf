            rdf:resource="https://www.securite-routiere-az.fr/r/route-vicinale/#:~:text=Le%20terme%20route%20%C2%AB%20vicinale%20%C2%AB%20ou,%C2%BB%20et%20de%20%C2%AB%20chemins%20ruraux"/>
        <rdfs:seeAlso rdf:resource="https://fr.wikipedia.org/wiki/Voie_communale_en_France"/>
        <skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q86685550"/>
