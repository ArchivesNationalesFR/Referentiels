      <rico:generalDescription rdf:parseType="Literal">
         <html:div xml:lang="fr">
            <h4>Organisation interne ou généalogie</h4>
            <html:p>
               <html:b>Organisation interne</html:b>
            </html:p>
            <html:p>
               <html:b>Directeur de cabinet</html:b>
            </html:p>
            <html:ul>
               <html:li>Olivier Schrameck (2 juin 1997 - 6 mai 2002)</html:li>
            </html:ul>
            <html:p>
               <html:b>Directeur</html:b>-<html:b>adjoint de cabinet</html:b>
