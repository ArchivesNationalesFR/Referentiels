      <skos:hiddenLabel xml:lang="fr">Chartière (rue)</skos:hiddenLabel>
      <rico:isOrWasDescribedBy rdf:resource="record/FRAN_RI_025-d3nz9fq4c7--tey8uq6zildv"/>
