   <rico:Place rdf:about="place/FRAN_RI_025-d3nz8xief2-1jzrwqtwb8t8u">
      <skos:prefLabel xml:lang="fr">boulevard de Bonne Nouvelle (Paris, France)</skos:prefLabel>
      <skos:altLabel xml:lang="fr">boulevard de Bonne Nouvelle</skos:altLabel>
      <rico:name xml:lang="fr">boulevard de Bonne Nouvelle (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Bonne-Nouvelle (boulevard de)</skos:hiddenLabel>
      <rico:isOrWasDescribedBy rdf:resource="record/FRAN_RI_025-d3nz8xief2-1jzrwqtwb8t8u"/>
