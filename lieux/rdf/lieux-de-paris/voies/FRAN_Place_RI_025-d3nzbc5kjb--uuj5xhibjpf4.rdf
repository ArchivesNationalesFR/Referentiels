      <skos:altLabel xml:lang="fr">rue de la Harpe</skos:altLabel>
      <rico:name xml:lang="fr">rue de la Harpe (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Harpe (rue de la)</skos:hiddenLabel>
      <rico:hasOrHadPlaceName rdf:resource="placeName/FRAN_RI_025-d3nzbc5kjb--uuj5xhibjpf4-rue%20de%20la%20Harpe-1851-05-10"/>
