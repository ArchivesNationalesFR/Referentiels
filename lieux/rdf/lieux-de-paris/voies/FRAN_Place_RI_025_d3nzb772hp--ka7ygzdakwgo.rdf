      <rico:name xml:lang="fr">quai de Gesvres (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Gesvres (quai de)</skos:hiddenLabel>
      <rico:hasOrHadPlaceName rdf:resource="placeName/FRAN_RI_025-d3nzb772hp--ka7ygzdakwgo-quai%20de%20Gesvres-1868-04-02"/>
