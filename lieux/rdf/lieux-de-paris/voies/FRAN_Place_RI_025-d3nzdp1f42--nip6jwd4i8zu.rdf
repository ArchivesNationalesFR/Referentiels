      <rico:hasExtent rdf:resource="extent/FRAN_RI_025-d3nzdp1f42--nip6jwd4i8zu_numero_impair"/>
      <anf-onto:startingOnStreet xml:lang="fr">rue Sauval, rive paire.</anf-onto:startingOnStreet>
      <anf-onto:endingOnStreet xml:lang="fr">rue Clémence Royer, rive paire.</anf-onto:endingOnStreet>
