   <rico:Place rdf:about="place/FRAN_RI_025-d3nzdqcipc-a5pmy75qdr45">
      <skos:prefLabel xml:lang="fr">rue du Vieux Colombier (Paris, France)</skos:prefLabel>
      <skos:altLabel xml:lang="fr">rue du Vieux Colombier</skos:altLabel>
      <rico:name xml:lang="fr">rue du Vieux Colombier (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Vieux-Colombier (rue du)</skos:hiddenLabel>
      <rico:hasOrHadPlaceName rdf:resource="placeName/FRAN_RI_025-d3nzdqcipc-a5pmy75qdr45-rue%20du%20Vieux%20Colombier"/>
