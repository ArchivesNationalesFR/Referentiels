      <rico:isOrWasAffectedBy rdf:resource="activity/ouverture-FRAN_RI_025-d3nzd4swmu-kt2cy6n1v9w8_1970"/>
      <rico:hasExtent rdf:resource="extent/FRAN_RI_025-d3nzd4swmu-kt2cy6n1v9w8_largeur"/>
      <rico:hasExtent rdf:resource="extent/FRAN_RI_025-d3nzd4swmu-kt2cy6n1v9w8_longueur"/>
      <anf-onto:startingOnStreet xml:lang="fr">quais d'Orléans, et de Bourbon.</anf-onto:startingOnStreet>
      <anf-onto:endingOnStreet xml:lang="fr">quai aux Fleurs, au droit de la rue Jean du Bellay et de la rue du Cloître Notre-Dame.</anf-onto:endingOnStreet>
