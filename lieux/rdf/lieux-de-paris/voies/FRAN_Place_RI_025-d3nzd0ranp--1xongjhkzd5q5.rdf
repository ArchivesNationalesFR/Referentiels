   <rico:Place rdf:about="place/FRAN_RI_025-d3nzd0ranp--1xongjhkzd5q5">
      <skos:prefLabel xml:lang="fr">rue Saint-André des Arts (Paris, France)</skos:prefLabel>
      <skos:altLabel xml:lang="fr">rue Saint-André des Arts</skos:altLabel>
      <rico:name xml:lang="fr">rue Saint-André des Arts (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Saint-André-des-Arts (rue)</skos:hiddenLabel>
      <rico:hasOrHadPlaceName rdf:resource="placeName/FRAN_RI_025-d3nzd0ranp--1xongjhkzd5q5-rue%20Saint-Andr%C3%A9%20des%20Arts"/>
