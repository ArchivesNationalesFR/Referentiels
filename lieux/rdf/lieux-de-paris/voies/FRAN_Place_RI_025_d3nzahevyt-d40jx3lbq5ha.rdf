      <rico:name xml:lang="fr">rue de la Fontenelle (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Fontenelle (rue de la)</skos:hiddenLabel>
      <rico:hasOrHadPlaceName rdf:resource="placeName/FRAN_RI_025-d3nzahevyt-d40jx3lbq5ha-rue%20de%20la%20Fontenelle-1868-04-02"/>
