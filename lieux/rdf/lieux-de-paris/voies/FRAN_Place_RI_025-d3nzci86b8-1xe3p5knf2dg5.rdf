   <rico:Place rdf:about="place/FRAN_RI_025-d3nzci86b8-1xe3p5knf2dg5">
      <skos:prefLabel xml:lang="fr">rue Pierre au Lard (Paris, France)</skos:prefLabel>
      <skos:altLabel xml:lang="fr">rue Pierre au Lard</skos:altLabel>
      <rico:name xml:lang="fr">rue Pierre au Lard (Paris, France)</rico:name>
      <skos:hiddenLabel xml:lang="fr">Pierre-au-Lard (rue)</skos:hiddenLabel>
      <rico:isOrWasDescribedBy rdf:resource="record/FRAN_RI_025-d3nzci86b8-1xe3p5knf2dg5"/>
