      <rdfs:label xml:lang="fr">Opéra Comique (Paris, France)</rdfs:label>
      <skos:prefLabel xml:lang="fr">Opéra Comique (Paris, France)</skos:prefLabel>
      <skos:altLabel xml:lang="fr">Opéra Comique</skos:altLabel>
